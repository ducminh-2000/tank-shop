{
  "name": "Account",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true
    }
  },
  "properties": {
    "name": {
      "type": "string"
    },
    "dateOfBirth": {
      "type": "date"
    },
    "kind": {
      "type": "string"
    },
    "sex": {
      "type": "boolean"
    },
    "avatar": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "country": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "accessTokens": {
      "type": "hasMany",
      "model": "AccountToken",
      "foreignKey": "userId",
      "options": {
        "disableInclude": true
      }
    },
    "roles": {
      "type": "hasMany",
      "model": "Role",
      "foreignKey": "principalId",
      "through": "RoleMapping"
    },
    "sentences": {
      "type": "hasMany",
      "model": "sentences"
    },
    "health_chatbox": {
      "type": "hasMany",
      "model": "health_chatbox"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "SUPERADMIN",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "ADMIN",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
